<mxfile host="app.diagrams.net">
  <diagram name="Page-1">
    <mxGraphModel dx="1202" dy="724">
      <root>
        <!-- Actors and Lifelines -->
        <mxCell id="User" value="User" style="actor;shape=umlActor;" vertex="1" parent="1">
          <mxGeometry x="40" y="200" width="40" height="60" />
        </mxCell>
        
        <mxCell id="Listly" value="Listly" style="lifeline;whiteSpace=wrap;" vertex="1" parent="1">
          <mxGeometry x="160" y="180" width="120" height="500" />
        </mxCell>

        <mxCell id="Playlist" value="Playlist" style="lifeline;whiteSpace=wrap;" vertex="1" parent="1">
          <mxGeometry x="320" y="180" width="120" height="500" />
        </mxCell>

        <mxCell id="LinkedList" value="LinkedList" style="lifeline;whiteSpace=wrap;" vertex="1" parent="1">
          <mxGeometry x="480" y="180" width="120" height="500" />
        </mxCell>

        <mxCell id="Node" value="Node" style="lifeline;whiteSpace=wrap;" vertex="1" parent="1">
          <mxGeometry x="640" y="180" width="120" height="500" />
        </mxCell>

        <!-- Message Flows -->
        <!-- Add Song Sequence -->
        <mxCell id="m1" style="edgeStyle=straight;endArrow=block;entryX=0;entryY=0;exitX=1;exitY=0;" 
                edge="1" parent="1" source="User" target="Listly">
          <mxGeometry relative="1" as="geometry">
            <mxPoint x="200" y="220" as="sourcePoint" />
            <mxPoint x="220" y="220" as="targetPoint" />
          </mxGeometry>
          <mxCell value="addSong(songPath)" style="edgeLabel;" vertex="1" connectable="0" parent="m1" />
        </mxCell>

        <mxCell id="m2" style="edgeStyle=straight;endArrow=block;" edge="1" parent="1" source="Listly" target="Playlist">
          <mxGeometry relative="1" as="geometry">
            <mxPoint x="280" y="240" as="sourcePoint" />
            <mxPoint x="380" y="240" as="targetPoint" />
          </mxGeometry>
          <mxCell value="handleCommand('add')" style="edgeLabel;" vertex="1" connectable="0" parent="m2" />
        </mxCell>

        <mxCell id="m3" style="edgeStyle=straight;endArrow=block;" edge="1" parent="1" source="Playlist" target="LinkedList">
          <mxGeometry relative="1" as="geometry">
            <mxPoint x="440" y="260" as="sourcePoint" />
            <mxPoint x="540" y="260" as="targetPoint" />
          </mxGeometry>
          <mxCell value="add(song)" style="edgeLabel;" vertex="1" connectable="0" parent="m3" />
        </mxCell>

        <mxCell id="m4" style="edgeStyle=straight;endArrow=block;" edge="1" parent="1" source="LinkedList" target="Node">
          <mxGeometry relative="1" as="geometry">
            <mxPoint x="600" y="280" as="sourcePoint" />
            <mxPoint x="700" y="280" as="targetPoint" />
          </mxGeometry>
          <mxCell value="createNode()" style="edgeLabel;" vertex="1" connectable="0" parent="m4" />
        </mxCell>

        <!-- Playback Control -->
        <mxCell id="m5" style="edgeStyle=straight;endArrow=block;" edge="1" parent="1" source="User" target="Listly">
          <mxGeometry relative="1" as="geometry">
            <mxPoint x="200" y="340" as="sourcePoint" />
            <mxPoint x="220" y="340" as="targetPoint" />
          </mxGeometry>
          <mxCell value="play()" style="edgeLabel;" vertex="1" connectable="0" parent="m5" />
        </mxCell>

        <mxCell id="m6" style="edgeStyle=straight;endArrow=block;startArrow=open;" edge="1" parent="1" source="Playlist" target="Playlist">
          <mxGeometry relative="1" as="geometry">
            <mxPoint x="380" y="380" as="sourcePoint" />
            <mxPoint x="380" y="420" as="targetPoint" />
          </mxGeometry>
          <mxCell value="playCurrentSong()" style="edgeLabel;" vertex="1" connectable="0" parent="m6" />
        </mxCell>

        <!-- Return Messages -->
        <mxCell id="m7" style="edgeStyle=straight;endArrow=open;dashed=1;" edge="1" parent="1" source="Node" target="LinkedList">
          <mxGeometry relative="1" as="geometry" />
          <mxCell value="nodeCreated" style="edgeLabel;" vertex="1" connectable="0" parent="m7" />
        </mxCell>

        <mxCell id="m8" style="edgeStyle=straight;endArrow=open;dashed=1;" edge="1" parent="1" source="LinkedList" target="Playlist">
          <mxGeometry relative="1" as="geometry" />
          <mxCell value="songAdded" style="edgeLabel;" vertex="1" connectable="0" parent="m8" />
        </mxCell>

        <mxCell id="m9" style="edgeStyle=straight;endArrow=open;dashed=1;" edge="1" parent="1" source="Playlist" target="Listly">
          <mxGeometry relative="1" as="geometry" />
          <mxCell value="playbackStarted" style="edgeLabel;" vertex="1" connectable="0" parent="m9" />
        </mxCell>

        <!-- Activation Bars -->
        <mxCell id="a1" style="fillColor=#000000;strokeColor=none;shape=partialRectangle;" vertex="1" parent="Listly">
          <mxGeometry x="160.1" y="220" width="119.8" height="140" />
        </mxCell>

        <mxCell id="a2" style="fillColor=#000000;strokeColor=none;shape=partialRectangle;" vertex="1" parent="Playlist">
          <mxGeometry x="320.1" y="240" width="119.8" height="100" />
        </mxCell>

        <!-- Notes -->
        <mxCell id="n1" value="Playlist manages audio playback\nthrough Java Sound API" 
               style="text;html=1;strokeColor=none;fillColor=#ffffcc;align=left;" vertex="1" parent="1">
          <mxGeometry x="320" y="460" width="160" height="40" />
        </mxCell>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
